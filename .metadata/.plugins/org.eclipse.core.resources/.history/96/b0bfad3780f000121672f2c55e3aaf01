package com.mss.infrastucture.web.dtos.translators;

import com.mss.domain.models.Category;

public class CustomerTranslator extends Translator<com.mss.infrastructure.web.dtos.Customer, com.mss.domain.models.Customer> {

	@Override
	public Customer Translate(com.mss.infrastructure.web.dtos.Customer dto) {
		return new com.mss.domain.models.Customer(dto.getId(), dto.getName());
	}

}
