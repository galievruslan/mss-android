package com.mss.infrastucture.web.dtos.translators;

import com.mss.domain.models.Category;

public class CustomerTranslator extends Translator<com.mss.infrastructure.web.dtos.Category, com.mss.domain.models.Category> {

	@Override
	public Category Translate(com.mss.infrastructure.web.dtos.Category dto) {
		return new com.mss.domain.models.Category(dto.getId(), dto.getName(), dto.getParentId());
	}

}
