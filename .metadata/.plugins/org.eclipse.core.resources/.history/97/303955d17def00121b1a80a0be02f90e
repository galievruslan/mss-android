package com.mss.application.tasks;

import com.mss.android.infrastructure.data.IEntity;
import com.mss.android.infrastructure.data.IRepository;
import com.mss.infrastructure.web.WebRepository;
import com.mss.infrastructure.web.dtos.Dto;

import android.os.AsyncTask;

public class SyncTask<TDto extends Dto, TModel extends IEntity> extends AsyncTask<Void, Void, Boolean> {

	private WebRepository<TDto> webRepo;
	private IRepository<TModel> modelRepo;
	public SyncTask(WebRepository<TDto> webRepo, IRepository<TModel> modelRepo){
		this.webRepo = webRepo;
		this.modelRepo = modelRepo;
	}
	
	@Override
	protected Boolean doInBackground(Void... arg0) {
		// TODO Auto-generated method stub
		return null;
	}

}
